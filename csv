def find_choice_order(self):
        # Open the input_file in read mode and output_file in write mode
        with open('test_in.csv', 'r') as read_obj, \
                open('test_out.csv', 'w', newline='') as write_obj:
            # Create a csv.reader object from the input file object
            csv_reader = reader(read_obj)
            # Create a csv.writer object from the output file object
            csv_writer = writer(write_obj)

            # Skip Headers
            next(csv_reader, None)

            # Read each row of the input csv file as list
            for row in csv_reader:
                choice_order = []
                print(row)
                # Turn strings into accessible lists
                row[self.MENU_INDEX] = row[self.MENU_INDEX].split(',')
                row[self.CHOICE_INDEX] = row[self.CHOICE_INDEX].split(',')

                # Check if choice entry is empty
                if row[self.CHOICE_INDEX][0] == '':
                    # Assign 'null' to case with no choice
                    choice_order = 'null'
                    row.append(choice_order)
                else:
                    # If choices exist then iterate through choices
                    for choice in row[self.CHOICE_INDEX]:
                        # Find the index of the choice in the menu (and add 1 as indexing from 1 not 0)
                        # .index() throws ValueError if not found, if thrown then the choice was not in the menu
                        index = row[self.MENU_INDEX].index(choice) + 1
                        choice_order.append(index)
                    # Add the new choice order data to the row
                    row.append(', '.join(map(str, choice_order)))

                # Clean-up for output, removes brackets []
                row[self.MENU_INDEX] = ', '.join(map(str, row[self.MENU_INDEX]))
                row[self.CHOICE_INDEX] = ', '.join(map(str, row[self.CHOICE_INDEX]))

                # Add the updated row to the output file
                csv_writer.writerow(row)